<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mapper.XitongMapper">
	<select id="updatext" resultType="Xitong">
		<!-- select * from Xitong; -->
		update xitong set quanxian = #{quanxian} where id = #{id};
	</select>
	<select id="updastu" resultType="Xitong">
		<!-- select * from Xitong; -->
		update xitong set quanxian = #{quanxian} where id = #{id};
	</select>
	
	<select id="selectid" resultType="Xitong" parameterType="Xitong">
		select quanxian from xitong where id=#{id}
	</select>
</mapper>